  "kitty" : skhd -k "ctrl - a" && skhd -k "2"
]

cmd - 3 [
  "kitty" : skhd -k "ctrl - a" && skhd -k "3"
]

cmd - 4 [
  "kitty" : skhd -k "ctrl - a" && skhd -k "4"
]

cmd - 5 [
  "kitty" : skhd -k "ctrl - a" && skhd -k "5"
